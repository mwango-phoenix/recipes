<h1 class="pb-4">Recently Added</h1>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  
    <% if(recipeList != '') { %>
        <%  for(let count = 0; count < 20; count++ ){  %>
            <a href="/recipe/<%- recipeList[count]._id %>" class="col text-center category__link">
                <div class="category__img category__img--large shadow">
                    <img src="/uploads/<%- recipeList[count].image %>" alt="<%- recipeList[count].name %>" loading="lazy" />
                </div>
                <div class="pt-1"><%- recipeList[count].name %></div>
            </a>
        <% } %>
    <% }else { %>
        <p>No items.</p>
    <% } %>
  </div>